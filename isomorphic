def ismo(x,y):
    if len(x)!=len(y):
        return False
    for i in range(len(x)):
        count=0
        if x.count(x[i]) !=y.count(y[i]):
            return false
        return True
print(ismo("fool","poor"))
print(ismo("foo","bar"))
print(ismo("fry","sky"))
print(ismo("apples","apple"))
        
