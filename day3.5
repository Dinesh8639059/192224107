def minjumps(arr,l,h):
        if(h==l):
                return 0
        if(arr[l]==0):
                return float("inf")
        min=float("inf")
        for i in range(l+1,h+1):
                if(i<l+arr[l]+1):
                        jumps=minjumps(arr,i,h)
                        if(jumps!=float("inf") and jumps+1<min):
                                min=jumps+1
        return min
n=int(input("no of values"))
j=[]
for i in range(n):
    K=int(input("enter the num"))
    j.append(K)
arr=j
n=len(arr)
print("Minimum number of jumps to reach","end is",minjumps(arr,0,n-1))

